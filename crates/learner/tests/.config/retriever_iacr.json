{
    "name": "iacr",
    "base_url": "https://eprint.iacr.org",
    "source": "iacr",
    "pattern": "(?:^|https?://eprint\\.iacr\\.org/)(\\d{4}/\\d+)(?:\\.pdf)?$",
    "endpoint_template": "https://eprint.iacr.org/oai?verb=GetRecord&identifier=oai:eprint.iacr.org:{identifier}&metadataPrefix=oai_dc",
    "response_format": {
        "type": "xml",
        "strip_namespaces": true,
        "paths": {},
        "field_maps": {
            "title": {
                "paths": [
                    "OAI-PMH/GetRecord/record/metadata/dc/title"
                ]
            },
            "abstract": {
                "paths": [
                    "OAI-PMH/GetRecord/record/metadata/dc/description"
                ]
            },
            "authors": {
                "paths": [
                    "OAI-PMH/GetRecord/record/metadata/dc/creator"
                ]
            },
            "publication_date": {
                "paths": [
                    "OAI-PMH/GetRecord/record/metadata/dc/date"
                ]
            },
            "pdf_url": {
                "paths": [
                    "OAI-PMH/GetRecord/record/metadata/dc/identifier"
                ],
                "transform": {
                    "type": "Replace",
                    "pattern": "^https://eprint\\.iacr\\.org/",
                    "replacement": "https://eprint.iacr.org/"
                }
            }
        }
    },
    "headers": {
        "User-Agent": "learner/0.7.0 (https://github.com/yourusername/learner)",
        "Accept": "application/xml"
    }
}